@page
@model BlazorApp.Server.Pages.BlazorIndexModel
@{
    ViewData["Title"] = "Blazor App";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" data-ng-app="app" data-ng-controller="appController" data-ng-cloak>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>Blazor App</title>
    <base href="/" />


    <link href="/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" asp-append-version="true" />
    <link href="/lib/angular.js/angular-csp.css" rel="stylesheet" asp-append-version="true" />
    <link href="/lib/animate.css/animate.css" rel="stylesheet" asp-append-version="true" />

    <link href="/css/app.css" rel="stylesheet" asp-append-version="true" />
    <link href="BlazorApp.Client.styles.css" rel="stylesheet" asp-append-version="true" />


    <link rel="icon" type="image/png" href="favicon.png" />
    <link href="manifest.json" rel="manifest" />
    <link rel="apple-touch-icon" sizes="512x512" href="icon-512.png" />
    <link rel="apple-touch-icon" sizes="192x192" href="icon-192.png" />
</head>

<body  class="container-fluid">
    <div id="app" class="animate__animated animate__fadeInLeft">
        <svg class="loading-progress">
            <circle r="40%" cx="50%" cy="50%" />
            <circle r="40%" cx="50%" cy="50%" />
        </svg>
        <div class="loading-progress-text"></div>
    </div>

    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>


    <script src="_content/Microsoft.AspNetCore.Components.WebAssembly.Authentication/AuthenticationService.js" asp-append-version="true"></script>
    <script src="_framework/blazor.webassembly.js" asp-append-version="true"></script>
    <script src="/_framework/aspnetcore-browser-refresh.js" autostart="true" asp-append-version="true"></script>
    <script asp-append-version="true">navigator.serviceWorker.register('service-worker.js');</script>

    <script src="/lib/jquery/jquery.js" asp-append-version="true"></script>
    <script src="/lib/bootstrap/js/bootstrap.bundle.js" asp-append-version="true"></script>
    <script src="/lib/bootbox.js/bootbox.js" asp-append-version="true"></script>
    <script src="/lib/angular.js/angular.js" asp-append-version="true"></script>

    <script src="/js/app.js" asp-append-version="true"></script>

</body>

</html>

